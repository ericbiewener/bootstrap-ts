{
  "name": "web-scraper",
  "version": "0.0.1",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "repository": "https://github.com/ericbiewener/web-scraper",
  "author": "Eric Biewener <ebiewener@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "tsc",
    "start": "ts-node src/main.ts",
    "debug": "ts-node --nolazy --inspect-brk=9229 src/main.ts",
    "prettier": "prettier --write \"**/*.{ts,tsx,js}\"",
    "eslint": "eslint --fix --ext .ts,.tsx ./src",
    "test": "jest",
    "test:watch": "yarn run test --watch"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-typescript": "7.10.1",
    "@types/cli-progress": "3.7.0",
    "@types/jest": "26.0.3",
    "@types/node": "14.0.14",
    "@types/progress-stream": "2.0.0",
    "@types/puppeteer": "3.0.1",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "babel-eslint": "11.0.0-beta.2",
    "babel-jest": "26.1.0",
    "eslint": "7.3.1",
    "eslint-config-ericbiewener-typescript": "5.13.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "lint-staged": {
    "**/*.{json,yml}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx,gql}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "cli-progress": "3.8.2",
    "got": "11.3.0",
    "jsdom": "16.2.2",
    "log-all-the-things": "5.0.5",
    "progress-stream": "2.0.0",
    "puppeteer": "4.0.1",
    "utlz": "2.6.0"
  }
}
